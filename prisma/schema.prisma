generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model City {
  name               String   @id
  current_temp       Int
  current_feels_like Int
  current_humidity   Int
  lat Decimal
  lon Decimal
  created_at         DateTime @default(now())
  updated_at         DateTime
}
